{
  "name": "@fleek-packages/database",
  "version": "1.0.0",
  "private": true,
  "main": "dist/fleek-packages-database.cjs.js",
  "module": "dist/fleek-packages-database.esm.js",
  "bin": {
    "database-password": "bin/database-password.ts",
    "database-url": "bin/database-url.ts",
    "skeema": "fleekdb/skeema"
  },
  "exports": {
    ".": {
      "types": "./dist/fleek-packages-database.cjs.js",
      "module": "./dist/fleek-packages-database.esm.js",
      "default": "./dist/fleek-packages-database.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "tsc": "tsc",
    "db:codegen": "DATABASE_URL=$(pnpm database-url production) pnpm kysely-codegen --dialect mysql --out-file src/db-codegen.ts",
    "ci": "npm-run-all -l tsc"
  },
  "preconstruct": {
    "exports": true,
    "entrypoints": [
      "index.ts"
    ],
    "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
      "importsConditions": true
    }
  },
  "dependencies": {
    "@fleek-packages/config": "workspace:^",
    "aws-sdk": "^2.1525.0",
    "kysely": "^0.26.3",
    "mysql2": "^3.6.5",
    "tsx": "^4.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "kysely-codegen": "^0.11.0"
  }
}
