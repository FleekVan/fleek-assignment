{
  "name": "fleek-assignment",
  "author": "Ivan Tanev <van.tanev@gmail.com>",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=8.12.0"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "preconstruct dev",
    "db:push:production": "pnpm skeema push production && pnpm run --filter=@fleek-packages/database db:codegen",
    "db:diff:production": "pnpm skeema diff production",
    "db:pull:production": "pnpm skeema pull production && pnpm run --filter=@fleek-packages/database db:codegen",
    "ci": "turbo ci"
  },
  "dependencies": {
    "@fleek-packages/database": "workspace:^",
    "@preconstruct/cli": "^2.8.2",
    "@types/node": "^20.10.5",
    "husky": "^8.0.3",
    "jsx": "^0.9.89",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "preconstruct": {
    "packages": [
      "packages/*",
      "!packages/config"
    ],
    "___experimentalFlags_WILL_CHANGE_IN_PATCH": {
      "importsConditions": true
    }
  }
}
